########################################################################################################################
# .bash_profile
#   ログインシェル起動時に設定ファイルを読み込む
########################################################################################################################

if [ -f "$HOME/.profile" ]; then . "$HOME/.profile"; fi
if [ -f "$HOME/.bashrc" ]; then . "$HOME/.bashrc"; fi
if [ -f "$HOME/.bashrc_local" ]; then . "$HOME/.bashrc_local"; fi
